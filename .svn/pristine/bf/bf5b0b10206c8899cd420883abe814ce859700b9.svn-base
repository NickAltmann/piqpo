A:link    { color: #444488; text-decoration: none; font-weight: bold}

A:visited { color: #444488; text-decoration: none; font-weight: bold} 

A:active  { color: #444488; text-decoration: none; font-weight: bold}

A:hover   { color: #888888; text-decoration: underline; font-weight: bold} 

body		{
				background-color: #FFFFFF;
                font-family: arial, sans-serif;
			}

hr
			{
				color: #444;
				background-color: #444;
				height: 5px;
				width: 80%;				
			}
			
.top        {
                margin: 0 auto;
				width: 800px;                
            }
.main		{
				width: 100%;
				text-align: center;
				float: left;
			}
.form_item	{
				width: 100%;
				float: left;
			}
			
.form_prompt{
				width: 200px;
				float: left;
				text-align: right;
				padding: 1px;
			}
			
.form_input	{
				float: left;
				padding: 1px;
				text-align: left;
			}
input       {
                width: 300px;
            }    
button      {
                width: 150px;
            }    

ul
			{
				text-align: left;
				list-style-type: none;
				padding: 0px;
				margin: 0px;
			}

		
.header 		{
				position:relative;
				width: 100%;
				text-align: center;
			}
						
			
.content	{
				padding-top: 5px;
				text-align: center;
				float: left;
				width: 100%;
			}
			
.block		{
				width: 100%;
				float: left;
			}
			
.section_title
			{
				text-align: left;
				font-style: italic;
				font-weight: bold;
			}
			
.section_content
			{
				text-align: center;
			}

